{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "Habit Tracker API for Node.js course",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --watch src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node src/db/seed.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:rollback": "drizzle-kit rollback",
    "test": "cross-env APP_STAGE=test vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "habit-tracker",
    "api",
    "nodejs",
    "express",
    "drizzle"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "custom-env": "^2.0.6",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jose": "^6.1.1",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "drizzle-kit": "^0.31.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
